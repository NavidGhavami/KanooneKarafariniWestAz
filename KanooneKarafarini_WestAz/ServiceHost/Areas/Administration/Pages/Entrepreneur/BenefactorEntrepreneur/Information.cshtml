@model EntrepreneurManagement.Application.Contract.BenefactorEntrepreneur.BenefactorEntrepreneurViewModel

@{
}

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h4 class="modal-title">کارآفرین خیّر: @Model.BenefactorName @Model.BenefactorFamily</h4>
</div>

<div class="modal-body">

    <table class="table table-bordered table-responsive">

        <thead>

            <tr>

                <th>#</th>
                <th>نام</th>
                <th>نام خانوادگی</th>
                <th>کد ملی</th>
                <th>شغل خیّر</th>
                <th>نوع همکاری</th>
                <th>تلفن</th>
                <th>موبایل</th>
                <th>آدرس</th>
                <th>توضیجات نوع همکاری</th>
                <th>تاریخ ثبت نام</th>


            </tr>


        </thead>

        <tbody>

            <tr>

                <td>@Model.Id</td>
                <td>@Model.BenefactorName</td>
                <td>@Model.BenefactorFamily</td>
                <td>@Model.BenefactorNationalId</td>
                <td>@Model.BenefactorJob</td>
                <td>@Model.BenefactorCooperation</td>
                <td>@Model.BenefactorPhone</td>
                <td>@Model.BenefactorMobile</td>
                <td>@Model.BenefactorAddress</td>
                <td>@Model.CooperationDescription</td>
                <td>@Model.CreationDate</td>

            </tr>
        



        </tbody>


    </table>

</div>
<div class="modal-footer">
    <button type="button" class="btn btn-default waves-effect" data-dismiss="modal">بستن</button>
</div>

